num1=float(input("please enter the first number:"))
operator=input("please enter the operator:")
num2=float(input("please enter the second number:"))

if operator=="+":
    print(num1+num2)
if operator=="-":
    print(num1-num2)
if operator=="*":
    print(num1*num2)
if operator=="/":
    print(num1/num2)
